{
    "manifest_version": 3,
    "name": "Link Saver",
    "description": "A simple Link Saver Chrome extension using React, TailwindCSS. Save your frequently used links without searching every time. (Mostly useful for job seekers)",
    "version": "1.0",
    "permissions": ["storage", "clipboardWrite"],
    "browser_action": {
      "default_popup": "index.html",
      "default_icon": {
        "16": "icons/16.png",
        "48": "icons/48.png",
        "128": "icons/128.png"
      }
    },
    "icons": {
      "16": "icons/16.png",
      "48": "icons/48.png",
      "128": "icons/128.png"
    },
 
     "browser_specific_settings": {
